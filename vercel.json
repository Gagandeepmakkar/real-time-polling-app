{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@vercel/node" },
      { "src": "public/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/register", "dest": "/server.js" },
      { "src": "/api/login", "dest": "/server.js" },
      { "src": "/socket.io/(.*)", "dest": "/server.js" },
      { "src": "/(.*)", "dest": "/public/$1" }
    ]
  }
  